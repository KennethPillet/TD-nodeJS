{
    "rules": {
      "arrow-parens": false,
      "arrow-return-shorthand": [true, "multiline"],
      "await-promise": [true, "PromiseLike", "Bluebird", "RequestPromise"],
      "ban": [
        true,
        "eval",
        { "name": ["describe", "only"], "message": "Do not focus tests" },
        { "name": ["it", "only"], "message": "Do not focus tests" }
      ],
      "ban-types": [true, ["object", "Use any instead"]],
      "binary-expression-operand-order": true,
      "callable-types": true,
      "class-name": true,
      "comment-format": [false, "check-space", "check-uppercase"],
      "curly": true,
      "cyclomatic-complexity": [true, 20],
      "deprecation": true,
      "encoding": true,
      "eofline": true,
      "forin": true,
      "import-spacing": true,
      "indent": [true, "spaces", 2],
      "interface-name": [true, "never-prefix"],
      "interface-over-type-literal": true,
      "label-position": true,
      "linebreak-style": [false],
      "match-default-export-name": true,
      "max-line-length": {
        "options": [120]
      },
      "member-access": true,
      "newline-before-return": false,
      "new-parens": true,
      "no-angle-bracket-type-assertion": true,
      "no-arg": true,
      "no-bitwise": true,
      "no-conditional-assignment": true,
      "no-consecutive-blank-lines": [true, 2],
      "no-console": {
        "options": ["debug", "info", "time", "timeEnd", "trace"]
      },
      "no-construct": true,
      "no-debugger": true,
      "no-duplicate-imports": true,
      "no-duplicate-super": true,
      "no-duplicate-switch-case": true,
      "no-duplicate-variable": true,
      "no-empty": true,
      "no-empty-interface": true,
      "no-eval": true,
      "no-import-side-effect": true,
      "no-inferrable-types": [true, "ignore-params"],
      "no-internal-module": true,
      "no-invalid-template-strings": true,
      "no-invalid-this": true,
      "no-irregular-whitespace": true,
      "no-misused-new": true,
      "no-non-null-assertion": false,
      "no-reference": true,
      "no-reference-import": true,
      "no-require-imports": true,
      "no-return-await": true,
      "no-shadowed-variable": true,
      "no-string-literal": true,
      "no-string-throw": true,
      "no-switch-case-fall-through": true,
      "no-this-assignment": true,
      "no-trailing-whitespace": [true, "ignore-comments"],
      "no-unnecessary-class": true,
      "no-unused-expression": true,
      "no-unnecessary-initializer": true,
      "no-var-keyword": true,
      "number-literal-format": true,
      "object-literal-key-quotes": [true, "as-needed"],
      "object-literal-sort-keys": false,
      "one-line": [
        true,
        "check-catch",
        "check-else",
        "check-finally",
        "check-open-brace",
        "check-whitespace"
      ],
      "one-variable-per-declaration": true,
      "only-arrow-functions": false,
      "ordered-imports": true,
      "prefer-const": true,
      "prefer-for-of": true,
      "prefer-template": [true, "allow-single-concat"],
      "prefer-object-spread": true,
      "promise-function-async": false,
      "quotemark": [true, "single", "avoid-escape"],
      "radix": true,
      "semicolon": [true, "always"],
      "space-before-function-paren": [
        true,
        {
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "trailing-comma": [
        true,
        {
          "multiline": "always",
          "singleline": "never"
        }
      ],
      "triple-equals": [true, "allow-null-check"],
      "typedef": [true, "call-signature", "parameter", "property-declaration"],
      "typedef-whitespace": [
        true,
        {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace",
          "variable-declaration": "nospace"
        }
      ],
      "unified-signatures": true,
      "use-isnan": true,
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-leading-underscore"
      ],
      "whitespace": [
        true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-module",
        "check-separator",
        "check-rest-spread",
        "check-type",
        "check-typecast",
        "check-preblock"
      ]
    },
    "jsRules": {
      "max-line-length": {
        "options": [120]
      }
    }
  }
  